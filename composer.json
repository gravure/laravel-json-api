    },
    "autoload-dev": {
        "psr-4": {
            "Gravure\\Api\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "^3.4",
        "phpunit/phpunit": "^6.0",
        "orchestra/database": "^v3.4.0-BETA1"
    }
}
