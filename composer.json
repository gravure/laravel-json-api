    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Gravure\\Api\\": "src/"
        }
    },
    "require": {
        "php": ">=7.1.0",
        "tobscure/json-api": "^0.3.0",
        "illuminate/routing": "5.4.*",
